        <Div id="BorderDiv" taborder="1" left="1.64%" top="16" right="2.29%" height="163" style="border:2 solid #90a4aeff ;">
          <Layouts>
            <Layout>
              <Div id="MainDiv" taborder="0" left="1.2%" top="7" right="3.43%" height="139" style="background:#eceff1ff;">
