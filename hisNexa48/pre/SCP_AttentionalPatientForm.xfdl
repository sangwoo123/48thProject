/**************************************************************************************************
*                                         공통 Script Include                                     *
***************************************************************************************************/

include "scripts::commonScripts.xjs";
include "scripts::wonScripts.xjs";

/**************************************************************************************************
*                                         Form Onload                                   *
***************************************************************************************************/
this.SCP_AttentionalPatientForm_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.dsAttentionalField.clearData();
	this.gfnService("findAttentionalFieldList","false");
}
/**************************************************************************************************
*                                         전역변수                                  *
***************************************************************************************************/
var nRow;

/***************************************************************************************************
*                                          버튼 EVENT START                                       *
/*-------------------------------------------------------------------------------------------------+
>>  click EVENT
+-------------------------------------------------------------------------------------------------*/

this.clickBtn = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	switch(obj){ 
		case this.searchPtBtn:			   // 관심환자등록을 위한 환자 조회
			this.clickSearchPtBtn();  	   
			break;
		case this.searchPtBtnForSch: 	   // 관심환자조회를 위한 환자 조회
			this.clickSearchPtBtnForSch();
			break;
		case this.searchAttBtn: 	       // 관심환자 조회
			this.clickSearchAttBtn();
			break;
		case this.regAttBtn:  		       // 관심환자 등록
			this.clickRegAttBtn();
			break;
		case this.clearAttBtn: 	           // 관심환자 등록 시 데이터 초기화
			this.clickClearAttBtn();
			break;
		case this.addAttFieldBtn:    	   // 관심영역 행추가
			this.clickAddAttFieldBtn();
			break;
		case this.deleteAttFieldBtn:  	   // 관심영역 행삭제
			this.clickDeleteAttFieldBtn();
			break;
		case this.saveAttFieldBtn:	 	   // 관심영역 저장
			this.clickSaveAttFieldBtn();
			break;
		
	}
}


/*-------------------------------------------------------------------------------------------------+
>>  조회
+-------------------------------------------------------------------------------------------------*/
this.clickSearchPtBtn = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	nRow=this.dsRegAttentionalPatient.addRow();
	
	this.gfnPatPopup();
	this.setPatInfo = function(arrRtn){ 
	this.dsRegAttentionalPatient.setColumn(nRow,"patNo",arrRtn[0]);
	this.dsRegAttentionalPatient.setColumn(nRow,"patName",arrRtn[1]);                  
   }
}

this.clickSearchPtBtnForSch = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfnPatPopup();
	this.setPatInfo = function(arrRtn){ 
	this.attPatDiv.patNoEd.set_value(arrRtn[0]);
	this.attPatDiv.patNmEd.set_value(arrRtn[1]);                      
   }
}

this.clickSearchAttBtn = function () {

	// this.dsAttentionalPatient.clearData();
	alert("dd");
	var attPtNo=this.attPatDiv.attPtNoEd.value;
	
	alert(attPtNo);
	var argument = 'attPtNo='+attPtNo; ;
	var serviceRow = this.dsService.findRow("serviceID", "findAttentionalPatientList");
	this.dsService.setColumn(serviceRow,"argument",argument);
	this.gfnService("findAttentionalPatientList","false");
}

/*-------------------------------------------------------------------------------------------------+
>>  행 추가,삭제
+-------------------------------------------------------------------------------------------------*/
this.clickAddAttFieldBtn = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	this.dsAttentionalField.addRow();
	var attDiv=this.attFieldDiv.attDivEd.value;
	var rowIdx=this.dsAttentionalField.getRowCount();
	var attFieldName=this.attFieldDiv.attFieldCombo.value;
	this.dsAttentionalField.setColumn(rowIdx-1,"attentionalDiv",attDiv);
	this.dsAttentionalField.setColumn(rowIdx-1,"attentionalFieldSeq",rowIdx+1);
	this.dsAttentionalField.setColumn(rowIdx-1,"attentionalFieldName",attFieldName);
}
this.clickDeleteAttFieldBtn = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	this.dsAttentionalField.deleteRow(this.dsAttentionalField.rowposition);
}
/*-------------------------------------------------------------------------------------------------+
>>  저장
+-------------------------------------------------------------------------------------------------*/
this.clickRegAttBtn = function (){
	alert("저장");

	
	this.gfnService("registerAttentionalPatient");
}

this.clickSaveAttFieldBtn = function()
{
	
	this.gfnService("batchAttentionalFieldProcess");
	
}
this.clickClearAttBtn = function ()
{
	/*this.startDateCal.set_value(1);
	 this.SCP_regAttPatDiv.clear();
	 this.patNoEd.clear();
	 this.attFileldCombo.clear();
	 this.attDivCombo.clear();
	 this.startDateCal.clear();
	 this.endDateCal.clear(); */
}

