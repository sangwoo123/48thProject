>>  COMBO Event 
+-------------------------------------------------------------------------------------------------*/
this.SCP_regAttPatDiv_attFieldCombo_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.dsAttentionalField.filter("");
 	var attFieldName=this.SCP_regAttPatDiv.attFieldCombo.value;
	this.dsAttentionalField.filter("attentionalFieldName=='"+attFieldName+"'");
