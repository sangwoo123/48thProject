this.callbackFunc = function(svcID,errorCode,errorMsg){
   
   if(svcID == "empImage"){
   if (errorCode < 1) {
      trace("[" + svcID + "] 에러코드 : " + errorCode + "\n" + errorMsg);
      this.gfnErrorPopup(svcID, errorMsg);
   } else {
      // 트랜잭션 완료
      trace("===== " + svcID + " 트랜잭션 성공 =====\n");
      this.fn_imageSet();
   }
   
   }
}

