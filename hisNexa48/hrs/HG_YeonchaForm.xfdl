          <Layouts>
            <Layout width="1223" height="627">
              <Grid id="transferPatGrid" taborder="0" binddataset="dsYeoncha" useinputpanel="false" autofittype="col" oncellclick="transferPatGrid_oncellclick" left="13" top="19" width="1202" height="592">
