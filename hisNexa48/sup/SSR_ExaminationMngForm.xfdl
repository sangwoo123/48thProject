<?xml version="1.0" encoding="utf-8"?>
<FDL version="1.5">
  <TypeDefinition url="..\default_typedef.xml"/>
  <Form id="SSR_ExaminationMngForm" classname="SSP_ExaminationMngForm" left="0" top="0" width="1261" height="779" titletext="New Form">
