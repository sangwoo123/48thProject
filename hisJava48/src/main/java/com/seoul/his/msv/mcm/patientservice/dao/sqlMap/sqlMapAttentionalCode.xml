<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seoul.his.msv.mcm.patientservice.dao.AttentionalCodeDAO">
	<cache flushInterval="86400000" eviction="LRU" />

	<resultMap type="AttentionalCodeBean" id="AttentionalCode">

		<result property="attentionalCode" column="ATTENTIONAL_CODE" />
		<result property="attentionalName" column="ATTENTIONAL_NAME" />

 	</resultMap>

 	<select id="selecAttentionalCodeList" flushCache="false"  useCache="true" resultMap="AttentionalCode" >

		SELECT    ATTENTIONAL_CODE
					, ATTENTIONAL_NAME
		FROM     ATTENTIONAL_CD

 	</select>
</mapper>